{
  "name": "pv-dynamodb-leveldown",
  "description": "A LevelDOWN API implementation of AWS DynamoDB.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "pretest": "tsc && cp -R ./types ./dist/lib/types",
    "test": "./test.sh",
    "posttest": "./testPost.sh",
    "climate": "./climate.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pontus-vision/DynamoDb-LevelDown.git"
  },
  "author": "Gio Palacino <gio@palacino.net>",
  "contributors": [
    "Leo Martins <lmartins@pontusvision.com"
  ],
  "files": [
    "dist",
    "package.json",
    "yarn.lock"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "database",
    "db",
    "dynamo",
    "dynamodb",
    "storage",
    "store",
    "level",
    "leveldb",
    "levelup",
    "leveldown",
    "pouchdb"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@types/levelup": "^5.1.0",
    "abstract-leveldown": "^7.2.0",
    "aws-sdk": "^2.1053.0",
    "level-supports": "^4.0.0",
    "lodash": "^4.17.21",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/abstract-leveldown": "^7.2.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "^4.14.178",
    "@types/tape": "^4.13.2",
    "@types/through2": "^2.0.36",
    "faucet": "^0.0.1",
    "levelup": "^5.1.1",
    "nyc": "^15.1.0",
    "semantic-release": "^18.0.1",
    "source-map-support": "^0.5.21",
    "tap-junit": "^5.0.1",
    "tape": "^5.4.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "release": {
    "branch": "master",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "minor"
            },
            {
              "type": "level",
              "release": "minor"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "overhaul",
              "release": "major"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  },
  "version": "3.2.5"
}
